@{
}

@model List<StudentPortalWeb.Models.Entities.Student>

<h1>Student List</h1>

<table class="table">

    <thead>

        <tr>
            <th>Id</th>
            <th>name</th>
            <th>Email</th>
            <th> </th>

        </tr>

    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>

                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Email</td>
                <td>

                    <a>Edit</a>

                </td>

            </tr>
            
        }
    </tbody>


</table>