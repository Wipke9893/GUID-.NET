@model IEnumerable<DisplayStudentViewModel>

<h1>Student List</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>name</th>
            <th>Email</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Email</td>
                <td>
                    <a class="btn btn-primary" asp-controller="Students" asp-action="Edit" asp-route-id="@student.Id">Edit</a>
                    <a class="btn btn-primary" asp-controller="Students" asp-action="Display" asp-route-id="@student.Id">View Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>
